<!DOCTYPE html>
<html>
  <head>
    <title>Feedback page</title>
  </head>
  <body>
    <h1>How would you rate this experience?</h1>
    <% it.options.forEach((s, i) => { %>
        <form method='POST' action='/feedbacks/<%=i+1%>'>
          <input type='submit' value='<%=s%>'/>
        </form>
    <% }) %>
    <% it.options.forEach((s, i) => { %>
        <a href='/feedbacks/<%=i=1%>'><%=s%></a><br />
    <% }) %>
  </body>
</html>